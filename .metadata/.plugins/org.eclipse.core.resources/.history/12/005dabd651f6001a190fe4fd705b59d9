package com.test.spring.di.seven;

import java.util.ArrayList;

public class MyDB {

	public ArrayList<String> list() {
		
		//JDBC 코드를 사용해서 구성원 목록을 가져오면 된다.
		//그래서 알아서 정보를 아래의 리스트에 넣어줬다고 치자.
		ArrayList<String> list = new ArrayList<String>();
		
		
		//원래는 아래처럼 해주겠다는 말이다.
//		while(rs.next()) {
//			
//		}
		
		//텍스트 파일 내용 -> 배열 옮기기
		list.add("사과");
		list.add("삼성");
		list.add("스마트폰");
		
		return list;
	}

}
