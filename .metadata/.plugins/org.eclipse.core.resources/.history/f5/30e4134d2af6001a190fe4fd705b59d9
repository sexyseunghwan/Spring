package com.test.spring.di.two;

import java.util.Scanner;

public class Util {
	
	public String getName() {
		
		//여기서도 본인이 다 알아서 하는게 아니라 Scanner 객체에 의존하고 있다.
		//Ex02 -> Util -> Scanner
		
		//Util의 의존 객체
		Scanner scan = new Scanner(System.in);
		
		System.out.print("이름 : ");
		
		String name = scan.nextLine();
		
		return name;
	}
	
}
